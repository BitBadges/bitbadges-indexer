    "merkletreejs": "^0.3.9",
    "mongodb": "^6.7.0",
    "mongodb-client-encryption": "^6.0.1",
    "mongoose": "^8.4.1",
    "moralis": "^2.25.2",
    "multer": "^1.4.5-lts.1",
    "oauth": "^0.10.0",
    "passkit-generator": "^3.1.11",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-reddit": "^1.1.0",
    "passport-stripe": "^0.2.3",
    "prettier": "^3.2.5",
    "response-time": "^2.3.2",
