    "@babel/preset-typescript": "^7.23.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^29.5.10",
