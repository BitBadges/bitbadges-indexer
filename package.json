{
  "name": "bitbadges-indexer",
  "version": "1.0.0",
  "description": "Indexer for the bitbadges blockchain",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src --runInBand",
    "test:coverage": "jest ./src --coverage --runInBand ",
    "start": "react-app-rewired start",
    "build": "tsc --build ./src",
    "dev": "npm run build && node --experimental-specifier-resolution=node build/dist/indexer.js",
    "indexer-dev": "npm run dev",
    "indexer": "node --experimental-specifier-resolution=node build/dist/indexer.js",
    "setup": "npx ts-node src/setup/setup.ts",
    "setup-and-run": "npm run setup with-delete && npm run indexer-dev",
    "setup-and-run-bootstrap": "npm run setup && npm run bootstrap && npm run indexer",
    "bootstrap": "npx ts-node src/setup/bootstrap.ts",
    "fuzz": "npx ts-node src/testutil/fuzzbootstrap.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "docs": "npx @redocly/cli build-docs ./openapi.yml",
    "link": "npm uninstall bitbadgesjs-sdk && npm link bitbadgesjs-sdk",
    "unlink": "npm unlink bitbadgesjs-sdk && npm install bitbadgesjs-sdk",
    "prepare": "ts-patch install && typia patch"
  },
  "type": "module",
  "author": "",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@cosmjs/crypto": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.2",
    "@cosmjs/stargate": "^0.32.2",
    "@keplr-wallet/cosmos": "^0.12.46",
    "@mattrglobal/bbs-signatures": "^1.3.1",
    "@moralisweb3/common-evm-utils": "^2.23.2",
    "@sendgrid/mail": "^8.1.0",
    "@softstack/joi-bigint": "^1.1.1",
    "async-mutex": "^0.4.0",
    "axios": "^1.6.8",
    "bech32": "^2.0.0",
    "bip322-js": "^1.1.0",
    "bitbadgesjs-sdk": "^0.3.51",
    "blockin": "^1.3.9",
    "bs58": "^5.0.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord.js": "^14.12.1",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.7.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
